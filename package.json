		"@nextcloud/router": "^2.0.0",
		"@nextcloud/vue": "7.0.0",
		"vue": "^2.7.13",
		"vue-loader": "^15.10.0",
		"vue-material-design-icons": "^5.1.2",
		"vue-router": "^3.6.5"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
